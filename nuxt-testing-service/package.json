{
  "name": "nuxt-testing-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest"
  },
  "dependencies": {
    "nuxt": "^2.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^26.6.3",
    "babel-preset-vue-app": "^2.0.0",
    "jest": "^26.6.3",
    "lodash.clonedeep": "^4.5.0",
    "vue-jest": "^3.0.7"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/$1",
      "^@@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/app/$1",
      "^~~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,vue}"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  }
}
